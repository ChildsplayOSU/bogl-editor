{
  "name": "spiel-front",
  "version": "1.0.4",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/codemirror": "0.0.84",
    "@types/jest": "^24.9.0",
    "@types/node": "^12.12.25",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "bootstrap": "^4.4.1",
    "history": "^4.10.1",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^1.0.4",
    "latest-version": "^5.1.0",
    "prismjs": "^1.19.0",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-codemirror": "^1.0.0",
    "react-codemirror2": "^6.0.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "terminal-in-react": "^4.3.1",
    "webpack": "4.41.2"
  },
  "scripts": {
    "setup": "npm install && cd .. && git clone https://github.com/The-Code-In-Sheep-s-Clothing/Spiel-Lang.git && cd Spiel-Lang && stack build && stack install",
    "start": "PORT=5168 react-scripts start",
    "startProduction": "(react-scripts start &>/dev/null &) && spielserver 5174",
    "stopProduction": "react-scripts stop",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "build-electron": "mkdir build/src && mkdir build/resources && cp -r electron/. build/electron && cp -r resources/* build/resources/",
    "package": "electron-builder build --mac --win -c.extraMetadata.main=build/electron/main.js --publish never",
    "package-mac": "electron-builder build --mac -c.extraMetadata.main=build/electron/main.js --publish never"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^8.1.1",
    "typescript": "^3.8.3",
    "webpack-cli": "^3.3.12"
  },
  "build": {
    "appId": "noId",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "resources/spielserver"
    ],
    "directories": {},
    "extraResources": [
      "resources"
    ]
  }
}
